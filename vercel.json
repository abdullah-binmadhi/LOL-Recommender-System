{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "src/index.html", "use": "@vercel/static" },
    { "src": "analytics.html", "use": "@vercel/static" },
    { "src": "debug-analytics.html", "use": "@vercel/static" },
    { "src": "simple-analytics.html", "use": "@vercel/static" },
    { "src": "test-analytics.html", "use": "@vercel/static" },
    { "src": "test-data-loading.html", "use": "@vercel/static" },
    { "src": "test-data-access.html", "use": "@vercel/static" },
    { "src": "test-data-api.html", "use": "@vercel/static" },
    { "src": "data/champions.json", "use": "@vercel/static" },
    { "src": "src/data/champions.json", "use": "@vercel/static" },
    { "src": "src/data/questions.json", "use": "@vercel/static" },
    { "src": "src/data/champion_counters.json", "use": "@vercel/static" },
    { "src": "src/data/champion_guides.json", "use": "@vercel/static" },
    { "src": "src/data/champion_tips.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/analytics", "dest": "/analytics.html" },
    { "src": "/debug-analytics", "dest": "/debug-analytics.html" },
    { "src": "/simple-analytics", "dest": "/simple-analytics.html" },
    { "src": "/test-analytics", "dest": "/test-analytics.html" },
    { "src": "/test-data-loading", "dest": "/test-data-loading.html" }
  ],
  "github": {
    "silent": true
  }
}